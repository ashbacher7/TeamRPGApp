<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TeamRPGApp.Choice2B">
    <ContentPage.Content>
        <StackLayout Padding="20"
                     Spacing="20">
            <Label Text="Choice B" 
               HorizontalOptions="Center"
               FontSize="24"/>
            <Image/>
            <Button Text="Bad"
                VerticalOptions="EndAndExpand"
                x:Name="PathB2A"
                    Clicked="ChoiceB1_Clicked" />
            <Button Text="Selfish"
                x:Name="PathB2B"
                  Clicked="PathB2B_Clicked"/>
            
                    

        </StackLayout>
    </ContentPage.Content>
</ContentPage>