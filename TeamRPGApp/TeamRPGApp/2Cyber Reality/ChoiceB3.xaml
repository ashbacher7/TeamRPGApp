<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TeamRPGApp.Choice3B">
    <ContentPage.Content>
        <StackLayout Padding="20"
                     Spacing="20">
            <Button Text="Good"
                    VerticalOptions="EndAndExpand" 
                    x:Name="ChoiceBG"
                Clicked="ChoiceBG_Clicked"
              />


            <Button Text="Selfish"
                    x:Name="ChoiceBS"
                    Clicked="ChoiceBS_Clicked"  
                    />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>